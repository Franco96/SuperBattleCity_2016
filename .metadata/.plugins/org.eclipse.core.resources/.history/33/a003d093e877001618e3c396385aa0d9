package BattleCity;

import javax.swing.ImageIcon;

public abstract class Enemigo extends Tanque {

	protected Enemigo(int velocidad, int x, int y) {
		super(velocidad, x, y);
		this.image[0] = new ImageIcon(this.getClass().getResource("/BattleCity/tanqueArribaQuieto.png"));
		this.image[1] = new ImageIcon(this.getClass().getResource("/BattleCity/tanqueAbajoQuieto.png"));
		this.image[2] = new ImageIcon(this.getClass().getResource("/BattleCity/tanqueIzquierdaQuieto.png"));
		this.image[3] = new ImageIcon(this.getClass().getResource("/BattleCity/tanqueDerechaQuieto.png"));
	}
	

}
